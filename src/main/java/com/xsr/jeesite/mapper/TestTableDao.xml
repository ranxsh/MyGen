<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.xsr.jeesite.dao.TestTableDao">
  	<resultMap id="BaseResultMap" type="com.xsr.jeesite.entity.TestTable" >
  	    <id column="id" property="id" jdbcType="INTEGER" />
  	    <result column="value" property="value" jdbcType="VARCHAR" />
  	</resultMap>

    <sql id="Base_Column_List" >
        id,
        value
    </sql>

    <select id="selectByPrimaryKey" resultMap="BaseResultMap" parameterType="INTEGER" >
        select
        <include refid="Base_Column_List" />
        from test_table
        where id = #{id,jdbcType=INTEGER}
    </select>

	<!--The user defined SQL!-->
  	
  	
  	<!--The user defined SQL!-->
  	
  	<sql id="updateSql">
		update test_table T4 set 
            <trim prefix="" prefixOverrides=",">
            	,value = #{value}
            </trim>
    </sql>
  	
  	<sql id="joinTables">
    </sql>
  	
    <sql id="colList">
        <trim prefix="" prefixOverrides=",">
            , T4.value AS C44
            , T4.id AS C45
        </trim>
    </sql>
    
    <sql id="whereCase">
    	<where>
            <if test="id != null">and T4.id = #{id}</if>
            <if test="idGT != null">and T4.id > #{idGT}</if><if test="idLT != null">and #{idLT} > T4.id</if>
            <if test="value != null">and T4.value = #{value}</if>
            <if test="valueLK != null">and T4.value LIKE #{valueLK}</if>
        </where>
    </sql>
    
     <sql id="smallWhereCase">
    	<where>
            <if test="id != null">and T4.id = #{id}</if>
            <if test="value != null">and T4.value = #{value}</if>
        </where>
    </sql>
    
    <resultMap id="returnEntity" type="TestTable">
        <id column="C45" javaType="Integer" jdbcType="INTEGER" property="id"/>
        <result column="C44" javaType="String" jdbcType="VARCHAR" property="value"/>
    </resultMap>
    
    <select id="get" parameterType="long" resultMap="returnEntity">
        select * from test_table T4 where  T4.id = #{id} and  true
    </select>
    
    <update id="update" parameterType="map">
        <include refid="updateSql"/>
        <include refid="whereCase"/>
    </update>
    
    <insert id="insertBatch"  parameterType="list">
     	insert into test_table 
     	(
            <trim prefix="" prefixOverrides=",">
            	,value
            </trim>
        ) values 
    	<foreach collection="list" item="item" index="index" separator=",">  
	    (
            <trim prefix="" prefixOverrides=",">
                 ,#{value}
            </trim>
	     )
    	</foreach>  
    </insert>  
    
    <insert id="insert" parameterType="TestTable" useGeneratedKeys="true" keyProperty="id" >
        insert into test_table 
        (
            <trim prefix="" prefixOverrides=",">
            	,value
            </trim>
        )values(
            <trim prefix="" prefixOverrides=",">
            	,#{value}
            </trim>
        )
    </insert>
    
    <delete id="remove" parameterType="map">
        delete T4 from test_table T4 <include refid="whereCase"/>
    </delete>
    
    <select id="find" parameterType="map" resultMap="returnEntity">
        select <include refid="colList" /> from test_table T4
		<include refid="joinTables"/>
		<include refid="whereCase"/>
        <if test="orderBy != null">order by #{orderBy}</if>
        <if test="limitStart != null and limitEnd != null">limit #{limitStart}, #{limitEnd}</if>
    </select>
    
    <select id="count" parameterType="map" resultType="long">
        select count(1) from test_table T4
		<include refid="joinTables"/>
        <include refid="whereCase"/>
    </select>
    
    <update id="updateByEntity" parameterType="TestTable">
        <include refid="updateSql"/>
        <include refid="smallWhereCase"/>
    </update>
        
    <delete id="removeByEntity" parameterType="TestTable">
        delete T4 from test_table T4 <include refid="smallWhereCase"/>
    </delete>
    
    <select id="findByEntity" parameterType="TestTable" resultMap="returnEntity">
        select <include refid="colList" /> from test_table T4
		<include refid="joinTables"/>
        <include refid="smallWhereCase"/>
        <if test="orderBy != null">order by #{orderBy}</if>
        <if test="limitStart != null and limitEnd != null">limit #{limitStart}, #{limitEnd}</if>
    </select>
    
    <select id="countByEntity" parameterType="TestTable" resultType="long">
        select count(1) from test_table T4
		<include refid="joinTables"/>
        <include refid="smallWhereCase"/>
    </select>
    
    
</mapper>